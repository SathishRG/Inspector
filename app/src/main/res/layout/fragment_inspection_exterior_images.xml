<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/warmGrey01"
        tools:context=".ui.login.fragments.inspection.InspectionExteriorImagesFragment">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="16dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="16dp">

                <!-- 1st row-->
                <androidx.appcompat.widget.AppCompatButton
                    style="@style/SellerInfo.Value"
                    android:id="@+id/inspection_add_exterior_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/tab_bg"
                    android:text="@string/add_exterior_images"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/car_front_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/front"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/inspection_add_exterior_btn" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/car_front"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/row1_barrier">

                    <ImageView
                        android:id="@+id/car_front_image_view"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/front_side3x" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/car_passenger_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/passenger_side"
                    style="@style/Login.Label"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/inspection_add_exterior_btn" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/car_passenger_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/row1_barrier">

                    <ImageView
                        android:id="@+id/car_passenger_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/passenger_side" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/driver_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/driver_side"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/inspection_add_exterior_btn" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/driver_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/row1_barrier">

                    <ImageView
                        android:id="@+id/driver_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/driver_side" />
                </androidx.cardview.widget.CardView>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row1_guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.0001" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row1_guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.3333" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row1_guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.6666" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row1_guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.9999" />

                <androidx.constraintlayout.widget.Barrier
                    android:id="@+id/row1_barrier"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:barrierDirection="bottom"
                    app:constraint_referenced_ids="driver_side_title,car_passenger_title,car_front_title" />

                <!-- 1st row end-->

                <!-- 2nd row start-->
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/rear_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    app:layout_constraintEnd_toStartOf="@+id/row1_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row1_guideline1"
                    android:text="@string/rear_side"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toBottomOf="@+id/car_front" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/rear_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row2_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row2_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/row2_barrier">

                    <ImageView
                        android:id="@+id/rear_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/rear_side" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/upper_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/upper_side"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row2_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row2_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/car_passenger_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/upper_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row2_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row2_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/row2_barrier">

                    <ImageView
                        android:id="@+id/upper_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/upper_side" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/rear_driver_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/rear_driver_side"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row2_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row2_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/driver_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/rear_driver_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row2_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row2_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/row2_barrier">

                    <ImageView
                        android:id="@+id/rear_driver_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/ic_driver_side" />
                </androidx.cardview.widget.CardView>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row2_guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.0001" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row2_guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.3333" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row2_guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".6666" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row2_guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.9999" />

                <androidx.constraintlayout.widget.Barrier
                    android:id="@+id/row2_barrier"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:barrierDirection="bottom"
                    app:constraint_referenced_ids="rear_side_title,upper_side_title,rear_driver_side_title" />
                <!-- 2nd row end-->


                <!-- 3rd row start-->
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/rear_passenger_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    app:layout_constraintEnd_toStartOf="@+id/row3_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row3_guideline1"
                    android:text="@string/rear_passenger_side"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toBottomOf="@+id/rear_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/rear_passenger_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row3_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row3_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/row3_barrier">

                    <ImageView
                        android:id="@+id/rear_passenger_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/rear_passenger_side" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/under_carriage_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/undercarriage"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row3_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row3_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/upper_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/under_carriage"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row3_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row3_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/row3_barrier">

                    <ImageView
                        android:id="@+id/under_carriage_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/under_carriage" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/under_carriage_left_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/undercarriage_nleft_side"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row3_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row3_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/rear_driver_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/under_carriage_left_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row3_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row3_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/row3_barrier">

                    <ImageView
                        android:id="@+id/under_carriage_left_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/undercarriage_left_side" />
                </androidx.cardview.widget.CardView>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row3_guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.0001" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row3_guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.3333" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row3_guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".6666" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row3_guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.9999" />

                <androidx.constraintlayout.widget.Barrier
                    android:id="@+id/row3_barrier"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:barrierDirection="bottom"
                    app:constraint_referenced_ids="rear_passenger_side_title,under_carriage_title,under_carriage_left_side_title" />
                <!-- 3rd row end-->

                <!-- 4th row start-->
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/under_carriage_right_side_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/row4_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row4_guideline1"
                    android:text="@string/undercarriage_nright_side"
                    style="@style/Login.Label"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toBottomOf="@+id/rear_passenger_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/under_carriage_right_side"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row4_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row4_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/row4_barrier">

                    <ImageView
                        android:id="@+id/under_carriage_right_side_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/undercarriage_right_side" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/under_carriage_front_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/undercarriage_nfront"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row4_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row4_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/under_carriage" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/under_carriage_front"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row4_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row4_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/row4_barrier">

                    <ImageView
                        android:id="@+id/under_carriage_front_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/undercarriage_front" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/under_carriage_back_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/undercarriage_nback"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row4_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row4_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/under_carriage_left_side" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/under_carriage_back"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row4_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row4_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/row4_barrier">

                    <ImageView
                        android:id="@+id/under_carriage_back_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/undercarriage_back" />
                </androidx.cardview.widget.CardView>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row4_guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.0001" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row4_guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.3333" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row4_guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".6666" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row4_guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.9999" />

                <androidx.constraintlayout.widget.Barrier
                    android:id="@+id/row4_barrier"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:barrierDirection="bottom"
                    app:constraint_referenced_ids="under_carriage_right_side_title,under_carriage_front_title,under_carriage_back_title" />
                <!-- 4th row end-->
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/inspection_add_exterior_panel_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/tab_bg"
                    style="@style/SellerInfo.Value"
                    android:layout_marginTop="16dp"
                    android:text="@string/add_exterior_panel_images"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/under_carriage_right_side" />


                <!-- 5th row start-->
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/ds_front_panel_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/row5_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row5_guideline1"
                    android:text="@string/ds_front_panel"
                    style="@style/Login.Label"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toBottomOf="@+id/inspection_add_exterior_panel_btn" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/ds_front_panel"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row5_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row5_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/row5_barrier">

                    <ImageView
                        android:id="@+id/ds_front_panel_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/ds_front_panel" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/vin_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/vin"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row5_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row5_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/inspection_add_exterior_panel_btn" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/vin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row5_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row5_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/row5_barrier">

                    <ImageView
                        android:id="@+id/vin_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/vin" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/ds_front_door_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    style="@style/Login.Label"
                    android:text="@string/ds_front_door"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row5_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row5_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/inspection_add_exterior_panel_btn" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/ds_front_door"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row5_guideline4"
                    app:layout_constraintStart_toStartOf="@+id/row5_guideline3"
                    app:layout_constraintTop_toBottomOf="@+id/row5_barrier">

                    <ImageView
                        android:id="@+id/ds_front_door_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/ds_front_door" />
                </androidx.cardview.widget.CardView>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row5_guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.0001" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row5_guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.3333" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row5_guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".6666" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row5_guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.9999" />

                <androidx.constraintlayout.widget.Barrier
                    android:id="@+id/row5_barrier"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:barrierDirection="bottom"
                    app:constraint_referenced_ids="ds_front_panel_title,vin_title,ds_front_door_title" />
                <!-- 5th row end-->

                <!-- 6th row start-->
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/ds_rear_door_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/row6_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row6_guideline1"
                    android:text="@string/ds_rear_door"
                    style="@style/Login.Label"
                    android:textColor="@color/black"
                    app:layout_constraintTop_toBottomOf="@+id/ds_front_panel" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/ds_rear_door"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row6_guideline2"
                    app:layout_constraintStart_toStartOf="@+id/row6_guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/row6_barrier">

                    <ImageView
                        android:id="@+id/ds_rear_door_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/ds_rear_door" />
                </androidx.cardview.widget.CardView>

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/ds_rear_panel_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="4dp"
                    android:text="@string/ds_rear_panel"
                    style="@style/Login.Label"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/row6_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row6_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/vin" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/ds_rear_panel"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    app:cardElevation="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/row6_guideline3"
                    app:layout_constraintStart_toStartOf="@+id/row6_guideline2"
                    app:layout_constraintTop_toBottomOf="@+id/row6_barrier">

                    <ImageView
                        android:id="@+id/ds_rear_panel_img"
                        android:layout_width="wrap_content"
                        android:layout_height="80dp"
                        android:src="@drawable/ds_rear_panel" />
                </androidx.cardview.widget.CardView>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row6_guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.0001" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row6_guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.3333" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row6_guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".6666" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/row6_guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.9999" />

                <androidx.constraintlayout.widget.Barrier
                    android:id="@+id/row6_barrier"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:barrierDirection="bottom"
                    app:constraint_referenced_ids="ds_rear_door_title,ds_rear_panel_title" />
                <!-- 6th row end-->

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/exterior_panel_image_next_btn"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    style="@style/ButtonRedActive"
                    android:layout_marginBottom="25dp"
                    android:textSize="14sp"
                    android:text="@string/next"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ds_rear_door"
                    app:layout_constraintVertical_bias="0.0" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/exterior_panel_image_next_btn" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

        <ProgressBar
            android:id="@+id/progressBarAddExteriorImages"
            style="@style/Widget.AppCompat.ProgressBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:indeterminate="true"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>